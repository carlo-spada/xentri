---
import { getAuth } from '@clerk/astro/server';
import AppShell from '../../layouts/AppShell.astro';
import { StrategyLanding } from '../../components/strategy/StrategyLanding';

const { orgId } = await getAuth(Astro);
if (!orgId) {
  return Astro.redirect('/sign-in');
}
---

<AppShell title="Strategy">
  <StrategyLanding
    client:only="react"
    orgId={orgId}
  />
</AppShell>
